                <allowspecific>0</allowspecific>
                <group>offline</group>
                <model>Doku\Core\Model\Payment\KlikBcaCore</model>
            </klik_bca_core>
        </payment>
        <doku_bca_payment_config>
            <bca_klikpay_core>
                <title>BCA Klikpay (dokuhosted)</title>
                <description>BCA Klikpay Doku hosted payment method</description>
            </bca_klikpay_core>
            <klik_bca_core>
                <title>Klik BCA (dokuhosted)</title>
                <description>Klik BCA Doku hosted payment method</description>
            </klik_bca_core>
        </doku_bca_payment_config>
    </default>
</config>
